<section class="exercise">
  <form class="delete-form">
    <a href="/routine/{{id}}">
    <input type="hidden" value="{{id}}">
    
      <div class="exercise-details">
        <div class="routineID" style="visibility:hidden">{{id}}</div>
        <header>{{name}}</header>
      </div>
    </a>
    <div class="form-group">
      <button class="btn btn-primary" onclick="deleteRoutine({{id}})" type="submit">Delete</button>
    </div>
  </form>
</section>

